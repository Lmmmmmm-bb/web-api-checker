{
  "name": "web-api-checker",
  "type": "module",
  "version": "0.0.3",
  "packageManager": "pnpm@7.32.4",
  "description": "check web api support",
  "author": {
    "email": "lmmmmmm12138@gmail.com",
    "name": "_lmmmmmm"
  },
  "license": "MIT",
  "homepage": "https://github.com/Lmmmmmm-bb/web-api-checker",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lmmmmmm-bb/web-api-checker"
  },
  "keywords": [
    "web-api",
    "web-api-checker",
    "browser-api",
    "browser-api-checker"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "play": "pnpm --filter playground dev",
    "build": "tsup",
    "lint": "eslint . --fix",
    "release": "bumpp --commit --tag --push && pnpm publish --no-git-checks"
  },
  "devDependencies": {
    "@lmmmmmm/eslint-config": "0.1.0",
    "bumpp": "^9.1.0",
    "eslint": "^8.40.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  }
}
